{
  "name": "cmdk-lite",
  "version": "0.0.5",
  "description": "A lightweight, unopinionated, zero dependency command menu for React",
  "keywords": [
    "command",
    "menu",
    "react"
  ],
  "homepage": "https://github.com/vgarmes/cmdk-lite",
  "bugs": {
    "url": "https://github.com/vgarmes/cmdk-lite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vgarmes/cmdk-lite.github"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts",
    "release": "pnpm run build && changeset publish",
    "lint": "tsc",
    "test": "echo \"Error: no tests\" && exit 1",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "ci": "pnpm run check-format && pnpm run build"
  },
  "author": "Victor Garcia Mestre",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.6",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/react": "^18.3.3",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
